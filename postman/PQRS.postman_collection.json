				{
					"name": "Crear Instancia PQRS con Correlation",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Check if response status is 200",
									"if (pm.response.code === 201) {",
									"    // Get the response body as a string",
									"    var processInstanceId = pm.response.text().trim(); // Trim to remove any extra spaces or newlines",
									"",
									"    // Ensure the response is not empty and is a valid number",
									"    if (processInstanceId && !isNaN(processInstanceId)) {",
									"        pm.environment.set(\"processInstanceId\", processInstanceId);",
									"        console.log(\"Process Instance ID saved: \" + processInstanceId);",
									"    } else {",
									"        console.log(\"Invalid processInstanceId received: \" + processInstanceId);",
									"    }",
									"} else {",
									"    console.log(\"Request failed with status: \" + pm.response.code);",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
