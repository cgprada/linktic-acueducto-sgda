					"name": "Consultar Tarea Actual Instancia Proceso",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Check if response status is 200",
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"",
									"    // Ensure \"task-summary\" exists and contains at least one task",
									"    if (jsonData[\"task-summary\"] && jsonData[\"task-summary\"].length > 0) {",
									"        var firstTask = jsonData[\"task-summary\"][0]; // Get the first task",
									"",
									"        if (firstTask[\"task-id\"]) {",
									"            pm.environment.set(\"taskInstanceId\", firstTask[\"task-id\"]);",
									"            console.log(\"Task Instance ID saved: \" + firstTask[\"task-id\"]);",
									"        } else {",
									"            console.log(\"No valid task-id found in the response.\");",
									"        }",
									"    } else {",
									"        console.log(\"No active tasks found for processInstanceId: \" + pm.environment.get(\"processInstanceId\"));",
									"    }",
									"} else {",
									"    console.log(\"Request failed with status: \" + pm.response.code);",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
